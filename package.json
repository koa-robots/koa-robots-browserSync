{
  "name": "koa-robots-browsersync",
  "version": "0.0.1",
  "description": "koa-robots-browsersync",
  "author": "寒飞紫",
  "license": "MIT",
  "engines": {
    "node": ">=4.2.3"
  },
  "main": "./dist/index",
  "dependencies": {
    "browser-sync": "^2.11.0",
    "chokidar": "^1.4.2"
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-plugin-transform-es2015-destructuring": "^6.4.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.3.16",
    "babel-plugin-transform-es2015-parameters": "^6.3.13",
    "babel-plugin-transform-es2015-spread": "^6.4.0",
    "co-fs": "^1.2.0",
    "koa": "^1.1.2",
    "should": "^8.1.1",
    "supertest": "1.1.0"
  },
  "babel": {
    "plugins": [
      "transform-es2015-spread",
      "transform-es2015-parameters",
      "transform-es2015-destructuring",
      "transform-es2015-modules-commonjs"
    ]
  },
  "scripts": {
    "compile": "babel src -d dist",
    "prepublish": "npm run compile",
    "test": "npm run compile && mocha --reporter spec --require should --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/koa-robots/koa-robots-browserSync.git"
  }
}
